@model List<UserModel>

@{
    ViewData["Title"] = "User";
}

@foreach (var t in Model)
{
    <h1>
       User:
    </h1>
    <p>
        <strong>First Name:</strong> <code>@t.FirstName</code>
    </p>
    <p>
        <strong>Last Name:</strong> <code>@t.LastName</code>
    </p>
    <p>
        <strong>Email:</strong> <code>@t.Email</code>
    </p>
}
